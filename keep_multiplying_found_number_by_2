def findFinalValue(nums, original):
    """
    :type nums: List[int]
    :type original: int
    :rtype: int
    """

    i = 0
    while i != len(nums):
        if (nums[i] == original):
            original = 2 * original
            i = 0
        else:
            i += 1
    return original

def findFinalValue(nums, original):
    """
    :type nums: List[int]
    :type original: int
    :rtype: int
    """

    while original in nums:
        original *= 2
    return original

print(findFinalValue([8,19,4,2,15,3], 2))
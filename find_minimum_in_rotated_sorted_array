def findMin(nums):
        if len(nums) == 1:
            return nums[0]
        elif len(nums) == 2:
            return min(nums[0], nums[1])
        if len(nums)>=2:
            mid = len(nums)//2
            if nums[mid] > nums[-1]:
                return findMin(nums[mid:])
            else:
                return findMin(nums[:mid+1])

        
nums = [4, 5, 6, 7, 0, 1, 2]

print(findMin(nums))